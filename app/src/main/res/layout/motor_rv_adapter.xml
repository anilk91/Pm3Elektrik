<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="wrap_content"
                                                   android:layout_height="wrap_content">

    <TextView
            android:text="1BD01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvMotorTag"
            android:fontFamily="@font/roboto_medium"
            android:textColor="@android:color/black"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/tvMotorMCCYeri"
            app:layout_constraintBottom_toBottomOf="@+id/tvMotorMCCYeri"/>
    <TextView
            android:text="2.1 Arka Hamur MCC"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:id="@+id/tvMotorMCCYeri" app:layout_constraintStart_toEndOf="@+id/tvMotorTag"
            android:layout_marginStart="16dp"
            android:fontFamily="@font/roboto_medium" android:textColor="@android:color/black"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"/>
    <TextView
            android:text="30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvMotorGuc"
            app:layout_constraintTop_toTopOf="@+id/tvMotorMCCYeri"
            android:fontFamily="@font/roboto_medium"
            android:textColor="@android:color/black" app:layout_constraintBottom_toBottomOf="@+id/tvMotorMCCYeri"
            app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginStart="1dp"
    />
    <TextView
            android:text="1000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvMotorDevir"
            app:layout_constraintTop_toTopOf="@+id/tvMotorMCCYeri"
            android:fontFamily="@font/roboto_medium"
            android:textColor="@android:color/black" app:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="@+id/tvMotorMCCYeri"
    />
    <TextView
            android:text="KW"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" app:layout_constraintStart_toEndOf="@+id/tvMotorGuc"
            android:layout_marginStart="4dp"
            app:layout_constraintTop_toTopOf="@+id/tvMotorMCCYeri"
            app:layout_constraintBottom_toBottomOf="@+id/tvMotorMCCYeri" android:fontFamily="@font/roboto_medium"
            android:textColor="@android:color/black"/>
    <TextView
            android:text="d/d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="@+id/tvMotorMCCYeri"
            app:layout_constraintBottom_toBottomOf="@+id/tvMotorMCCYeri"
            app:layout_constraintStart_toEndOf="@+id/tvMotorDevir" android:layout_marginStart="4dp"
            android:fontFamily="@font/roboto_medium" android:textColor="@android:color/black"
    />
    <TextView
            android:text="("
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" app:layout_constraintStart_toEndOf="@+id/tvMotorMCCYeri"
            android:layout_marginStart="16dp" android:fontFamily="@font/roboto_medium"
            android:textColor="@android:color/black"
            app:layout_constraintTop_toTopOf="@+id/tvMotorMCCYeri"
            app:layout_constraintBottom_toBottomOf="@+id/tvMotorMCCYeri"/>
    <TextView
            android:text=")"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" app:layout_constraintStart_toEndOf="@+id/textView2"
            android:layout_marginStart="1dp"
            app:layout_constraintTop_toTopOf="@+id/tvMotorMCCYeri"
            app:layout_constraintBottom_toBottomOf="@+id/tvMotorMCCYeri" android:fontFamily="@font/roboto_medium"
            android:textColor="@android:color/black"/>
    <View
            android:layout_width="wrap_content"
            android:layout_height="2dp"
            android:id="@+id/view"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:background="@android:color/darker_gray"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tvMotorMCCYeri"/>
</androidx.constraintlayout.widget.ConstraintLayout>