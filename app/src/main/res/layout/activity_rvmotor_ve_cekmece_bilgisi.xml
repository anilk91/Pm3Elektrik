                    app:layout_constraintTop_toTopOf="@+id/MotorTag" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
            <TextView
                    android:text="@string/bilinmiyor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvGucKw" app:layout_constraintStart_toEndOf="@+id/NomTripAkimi"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvMotorTag"
                    android:fontFamily="@font/roboto_medium" android:textColor="@android:color/black"/>
            <TextView
                    android:text="KW"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/KW" app:layout_constraintStart_toEndOf="@+id/tvGucKw"
                    android:layout_marginStart="4dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvMotorTag" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
            <TextView
                    android:text="@string/bilinmiyor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvGucHp" app:layout_constraintStart_toEndOf="@+id/KW"
                    android:layout_marginStart="24dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvMotorTag" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
            <TextView
                    android:text="HP"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/GucHp" app:layout_constraintStart_toEndOf="@+id/tvGucHp"
                    android:layout_marginStart="4dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvMotorTag" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
            <TextView
                    android:text="@string/bilinmiyor"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvDevir" app:layout_constraintStart_toEndOf="@+id/NomTripAkimi"
