                    android:text="@string/bilinmiyor"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvInsaTipi" app:layout_constraintStart_toEndOf="@+id/NomTripAkimi"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvNomTripAkimi" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
            <TextView
                    android:text="@string/bilinmiyor"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvFlans" app:layout_constraintStart_toEndOf="@+id/NomTripAkimi"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvInsaTipi" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"/>
            <TextView
                    android:text="@string/bilinmiyor"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvMotorAdres" app:layout_constraintStart_toEndOf="@+id/NomTripAkimi"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvFlans" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
            <TextView
                    android:text="@string/bilinmiyor"
