                    app:layout_constraintTop_toBottomOf="@+id/tvMotorMCCYeri" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
