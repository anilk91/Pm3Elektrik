            android:layout_height="35dp" app:srcCompat="@drawable/close"
            android:id="@+id/imgClose" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"/>
