                    app:layout_constraintTop_toBottomOf="@+id/tvGucKw" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
            <TextView
                    android:text="@string/bilinmiyor"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvNomTripAkimi" app:layout_constraintStart_toEndOf="@+id/NomTripAkimi"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvDevir" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:fontFamily="@font/roboto_medium"
                    android:textColor="@android:color/black"/>
