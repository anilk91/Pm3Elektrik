<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content"
                                                   tools:context=".MotorListeSayfasi.EkleFragmentleri.ITSoftStarterEkle"
                                                   android:background="#E3F2FD">


    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="IT SoftStarter"
            android:ems="10"
            android:id="@+id/etITSoftStarter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:fontFamily="@font/nunito_extrabold" android:textSize="12sp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etITModel"
            app:layout_constraintEnd_toEndOf="@+id/etITSoftStarter"
            app:layout_constraintStart_toStartOf="@+id/etITSoftStarter" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/etITSoftStarter" android:hint="Model Seri No"
            android:fontFamily="@font/nunito_extrabold" android:textSize="12sp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/etITDegisimTarihi"
            app:layout_constraintEnd_toEndOf="@+id/etITModel" app:layout_constraintStart_toStartOf="@+id/etITModel"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/etITModel"
            android:hint="Değişim Tarihi" android:fontFamily="@font/nunito_extrabold" android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>