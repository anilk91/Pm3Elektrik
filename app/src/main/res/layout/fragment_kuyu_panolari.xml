<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F4F9FD"
        tools:context=".DigerBilgilerSayfasi.KuyuPanoSayfasi.KuyuPanolari">


    <ImageView
            android:id="@+id/imgKuyuPanolariLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0.1"
            android:src="@drawable/modern_karton_logo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <ImageView
            android:id="@+id/imgKuyuPanoClose"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/close"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imgKuyuPanoClose">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <androidx.cardview.widget.CardView
                    android:id="@+id/birNoluKuyu"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="16dp"
                    app:cardCornerRadius="15dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    <TextView
                            android:id="@+id/textView101"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="1 NOLU KUYU"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="14sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                            android:id="@+id/textView103"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="Adres :"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="12sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView101" />

                    <TextView
                            android:id="@+id/textView104"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:fontFamily="@font/nunito_semibold"
                            android:text="Mazot İstasyonu Yanında"
                            android:textColor="@android:color/black"
                            android:textSize="12sp"
                            app:layout_constraintStart_toEndOf="@+id/textView103"
                            app:layout_constraintTop_toTopOf="@+id/textView103" />

                    <TextView
                            android:id="@+id/textView105"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="Sürücü Tipi :"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="12sp"
                            app:layout_constraintStart_toStartOf="@+id/textView103"
                            app:layout_constraintTop_toBottomOf="@+id/textView103" />

                    <TextView
                            android:id="@+id/textView106"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="Pano Değ. Tarihi :"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/textView105"
                            app:layout_constraintTop_toBottomOf="@+id/textView105" />

                    <TextView
                            android:id="@+id/textView107"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:fontFamily="@font/nunito_semibold"
                            android:text="Yıldız - Üçgen Kontaktör"
                            android:textColor="@android:color/black"
                            android:textSize="12sp"
                            app:layout_constraintStart_toEndOf="@+id/textView105"
                            app:layout_constraintTop_toTopOf="@+id/textView105" />

                    <TextView
                            android:id="@+id/textView108"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:fontFamily="@font/nunito_semibold"
                            android:text="Bilgi Yok"
                            android:textColor="@android:color/black"
                            android:textSize="12sp"
                            app:layout_constraintStart_toEndOf="@+id/textView106"
                            app:layout_constraintTop_toTopOf="@+id/textView106" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                    android:id="@+id/ikiNoluKuyu"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    app:cardCornerRadius="15dp"
                    app:layout_constraintEnd_toEndOf="@+id/birNoluKuyu"
                    app:layout_constraintStart_toStartOf="@+id/birNoluKuyu"
                    app:layout_constraintTop_toBottomOf="@+id/birNoluKuyu">

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    <TextView
                            android:id="@+id/textView109"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="4 NOLU KUYU"
                            android:textColor="@android:color/holo_red_dark"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                            android:id="@+id/textView110"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="Adres :"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="12sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView109" />

                    <TextView
                            android:id="@+id/textView111"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="Sürücü Tipi :"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="12sp"
                            app:layout_constraintStart_toStartOf="@+id/textView110"
                            app:layout_constraintTop_toBottomOf="@+id/textView110" />

                    <TextView
                            android:id="@+id/textView112"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="8dp"
                            android:fontFamily="@font/nunito_extrabold"
                            android:text="Pano Değ. Tarihi :"
                            android:textColor="@android:color/holo_red_dark"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/textView111"
                            app:layout_constraintTop_toBottomOf="@+id/textView111" />

                    <TextView
                            android:id="@+id/textView113"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:fontFamily="@font/nunito_semibold"
                            android:text="Arıtmada"
                            android:textColor="@android:color/black"
                            android:textSize="12sp"
                            app:layout_constraintStart_toEndOf="@+id/textView110"
                            app:layout_constraintTop_toTopOf="@+id/textView110" />

                    <TextView
                            android:id="@+id/textView116"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:fontFamily="@font/nunito_semibold"
                            android:text="Yıldız - Üçgen Kontaktör"
                            android:textColor="@android:color/black"
                            android:textSize="12sp"
                            app:layout_constraintStart_toEndOf="@+id/textView111"
                            app:layout_constraintTop_toTopOf="@+id/textView111" />

                    <TextView
                            android:id="@+id/textView117"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:fontFamily="@font/nunito_semibold"
                            android:text="Bilgi Yok"
                            android:textColor="@android:color/black"
                            android:textSize="12sp"
                            app:layout_constraintStart_toEndOf="@+id/textView112"
                            app:layout_constraintTop_toTopOf="@+id/textView112" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>